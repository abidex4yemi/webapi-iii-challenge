  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/Node-Blog.git"
  },
  "keywords": [
    "node",
    "express",
    "blog",
    "api"
  ],
  "author": "Lambda School",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LambdaSchool/Node-Blog/issues"
  },
  "homepage": "https://github.com/LambdaSchool/Node-Blog#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "knex": "^0.16.3",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "knex-cleaner": "^1.1.4",
    "nodemon": "^1.18.10",
