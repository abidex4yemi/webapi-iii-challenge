{
  "name": "webapi-blog",
  "version": "1.0.0",
  "description": "Web API Blog Project",
  "main": "src/server/index.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/server",
    "server:prod": "node ./dist/server",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
